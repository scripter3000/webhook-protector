{
  "version": 2,
  "routes": [
    {
      "src": "^/dashboard$",
      "dest": "/dashboard.html"
    },
    {
      "src": "^/api/webhook/(.+)$",
      "dest": "/api/webhook/$1"
    },
    {
      "src": "^/api/create-webhook$",
      "dest": "/api/create-webhook"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@20.x"
    }
  }
}
